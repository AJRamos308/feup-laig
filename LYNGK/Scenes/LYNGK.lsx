        </DESCENDANTS>

    </NODE>

	<NODE id="placeCylinder7">
        <MATERIAL id="redMaterial" />
        <TEXTURE id="null" />

		<TRANSLATION x="0" y="0" z="27" />

        <DESCENDANTS>
