        </DESCENDANTS>

    </NODE>

	<NODE id="placeCylinder6">
        <MATERIAL id="redMaterial" />
        <TEXTURE id="null" />

		<TRANSLATION x="0" y="0" z="22.5" />

        <DESCENDANTS>
