        <TEXTURE id="null" />

		<TRANSLATION x="0" y="0.225" z="0" />
		<ROTATION axis="x" angle="90" />

        <DESCENDANTS>
       		<NODEREF id="cylinder1" />
			<NODEREF id="topCylinder1" />
			<NODEREF id="botCylinder1" />
        </DESCENDANTS>

    </NODE>

	<NODE id="cylinder1">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <DESCENDANTS>
			<LEAF id="cylinder" type="cylinder" args="0.45 0.5 0.5 20 20" />
        </DESCENDANTS>
    </NODE>

	<NODE id="topCylinder1">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

		<TRANSLATION x="0" y="0" z="0.45" />

        <DESCENDANTS>
			<LEAF id="cylinder" type="cylinder" args="0 0.5 0 20 20" />
        </DESCENDANTS>
    </NODE>

	<NODE id="botCylinder1">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

		<TRANSLATION x="0" y="0" z="0" />
